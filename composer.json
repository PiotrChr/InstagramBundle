    "keywords": ["instagram", "symfony"],
    "homepage": "https://github.com/VikramHauteLook/InstagramBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Vikram Jiandani",
